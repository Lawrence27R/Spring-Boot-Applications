spring.application.name=20-BatchProcessing
spring.datasource.url=jdbc:mysql://localhost:3306/batchdb
spring.datasource.username=root
spring.datasource.password=root
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQLDialect

spring.batch.job.enabled=false

spring.batch.jdbc.initialize-schema=always

spring.sql.init.schema-locations=classpath:org/springframework/batch/core/schema-mysql.sql

#spring.sql.init.schema-locations=classpath:org/springframework/batch/core/schema-sql