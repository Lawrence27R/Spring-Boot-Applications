package com.aurionpro.bankapp.service;

import java.util.Date;

import org.springframework.data.domain.Page;
import org.springframework.data.domain.Pageable;

import com.aurionpro.bankapp.dto.TransactionDto;

public interface TransactionService {

	Page<TransactionDto> getFilteredTransactions(Long accountNumber, Date startDate, Date endDate,
			String typeOfTransaction, Pageable pageable);

	Page<TransactionDto> getAllTransactions(Long accountNumber, Date startDate, Date endDate, String typeOfTransaction,
			Pageable pageable);
}
